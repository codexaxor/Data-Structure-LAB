#include <iostream>
#include <time.h>

using namespace std;

int bin[1000];
int limit;
int f;

void Input()
{
    printf("\nInput array Size : ");
    scanf("%d",&limit);
    
    bin[limit];
    
    for (int i = 0; i < limit; ++i)
    {
        scanf("%d",&bin[i]);
    }
    printf("\nInput your value to find : ");
    scanf("%d",&f);
}

void BinarySearch(int bin[], int n, int f)
{
    
    int first=0, last=n-1, mid=(first+last)/2;
    
    while(first<=last)
    {
        if (bin[mid]<f)
        {
            first=mid+1;
        }
        else if (bin[mid]>f)
        {
            last=mid-1;
        }
        else if (bin[mid]==f)
        {
            printf("Found!\n");
            break;
        }
        mid = (first+last)/2;
    }
    if (first>last)
    {
        printf("Not Found!\n");
    }
    
}


int main()
{
    clock_t begin,end;
    double RT;
    
    begin = clock();
    Input();
    BinarySearch(bin,limit,f);
    end = clock();
    RT = (double) (end-begin)/CLOCKS_PER_SEC;
    cout<<RT<<endl;
    return 0;
}

