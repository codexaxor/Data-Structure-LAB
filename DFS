#include <bits/stdc++.h>
using namespace std;

int color[1000];
vector<int>v[1000];
queue<int>q;

void DFS(int root){
    
    color[root] = 1;
    int x;
    for (int i=0; i<v[root].size(); i++) {
        
        x = v[root][i];
        
        if (color[x]==0) {
            DFS(x);
        }
    }
    color[root] = 2;  
}

int main(){

    int node,edge,root;
    
    cin>>node>>edge;
    
    for (int i=0; i<edge; i++) {
        int x,y;
        
        cin>>x>>y;
        
        v[x].push_back(y);
        v[y].push_back(x);
        
    }
    
    cin>>root;
    
    DFS(root);
    
    return 0;
}

