#include<bits/stdc++.h>
using namespace std;

int color[1000];
vector<int>v[1000];
queue<int>q;


void BFS(int root){
    
    color[root] = 1;
    q.push(root);
    
    
    while (!q.empty()) {
        
        int n = q.front();
        q.pop();
        
        int x;
        
        for (int i=0; i<v[n].size(); i++) {
            x= v[n][i];
            
            if (color[x] == 0) {
                q.push(x);
                color[x] = 1;
            }
        }
        
    }
    
}

int main(){

    int node,edge,root;
    
    cin>>node>>edge;
    
    for (int i=0; i<edge; i++) {
        int x,y;
        
        cin>>x>>y;
        
        v[x].push_back(y);
        v[y].push_back(x);
        
    }
    
    cin>>root;
    
    BFS(root);
    
    return 0;
}

